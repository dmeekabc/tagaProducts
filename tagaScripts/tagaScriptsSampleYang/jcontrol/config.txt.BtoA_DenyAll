security {
    policies {
      from-zone NETB to-zone NETA {
        policy deny-or-permit-All_NETB_to_NETA {
          match {
            source-address any;
            destination-address any;
            application any;
          } # match
          then {
              deny;
          } # then
        } # policy
      } # from-zone
    }  # policies
} # security
